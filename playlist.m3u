import requests
import os
from datetime import datetime

# URL playlist IPTV (bit.ly akan redirect)
PLAYLIST_URL = "https://bit.ly/BrFiDO7"

def fetch_playlist():
    # Ikuti redirect
    response = requests.head(PLAYLIST_URL, allow_redirects=True)
    final_url = response.url
    
    # Unduh konten playlist
    playlist = requests.get(final_url)
    playlist.raise_for_status()
    
    return playlist.text

def save_playlist(content):
    with open("playlist.m3u", "w", encoding="utf-8") as f:
        f.write(content)
    
    # Buat file baca juga
    with open("LAST_UPDATE.txt", "w") as f:
        f.write(f"Terakhir diupdate: {datetime.utcnow().strftime('%Y-%m-%d %H:%M:%S UTC')}")
        f.write(f"\nSumber: {PLAYLIST_URL}")

if __name__ == "__main__":
    content = fetch_playlist()
    save_playlist(content)
    print(f"Playlist diperbarui! {len(content)} karakter")#EXTINF:-1 tvg-logo="https://www.mncvision.id/userfiles/image/channel/sctv.png" group-title="INDONESIA",SCTV HD
